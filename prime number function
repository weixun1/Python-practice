def check_prime(number = int(input("Please enter a number\n"))):
    l = list(range(1,number + 1,1))
    divisor = 0
    
    for x in l:
        if((number % x) == 0):
            divisor = divisor + 1
            
    if(divisor == 2):
        print(number, "is a prime number\n")
    else:
        print(number, "is not a prime number \n")

check_prime()
